#myApp{
    width: 100%;
}
.myApp-top{
    width: 100%;
    height: auto;
    padding: 14px;
    overflow: hidden;
    border-bottom: 12px #F3F3F3 solid;
}
.myApp-top .header-img{
	height: 60px;
	float: left;
	display: block;
}
.myApp-top .myApp-top-content{
	display: block;
    float: left;
    padding-left: 5%;
}
.myApp-top .myApp-top-content .myApp-top-content-teacherName{
	font-size: 1.5rem;
    color: #333333;
    line-height: 30px;
}
.myApp-top .myApp-top-content .myApp-top-content-dept{
	font-size: 1.3rem;
	color: #666666;
    line-height: 30px;
}
.myApp-top .myApp-top-bianji{
	display: block;
    float: right;
    padding-right: 26px;
}
.myApp-top .myApp-top-bianji .myApp-top-bianji-p{
    line-height: 60px;
    color: #999999;
    font-size: 1.3rem;
}
.myApp-top .myApp-top-bianji .icon-youjiantou{
	font-size: 1.3rem;
	color: #999999;
}
.myApp-main{
    width: 100%;
    overflow: hidden;
    height: auto;
}
.myApp-main .myApp-main-ul{
    overflow: hidden;
    padding: 0 14px;
}
.myApp-main .myApp-main-ul .myApp-main-ul-item:last-of-type{
	border-bottom: 0;
}
.myApp-main .myApp-main-ul .myApp-main-ul-item {
    padding: 16px 0 ;
	border-bottom: 1px solid #F3F3F3;
}
.myApp-main .myApp-main-ul .myApp-main-ul-item .myApp-main-ul-item-icon{
    height: 16px;
    vertical-align: middle;
}
.myApp-main .myApp-main-ul .myApp-main-ul-item .myApp-main-ul-item-title{
    font-size: 1.4rem;
    font-weight: 200;
    color: #333333;
    margin-left: 14px;
    vertical-align: middle;
}
.myApp-main .myApp-main-ul .myApp-main-ul-item .icon-youjiantou{
    color: #999999;
    float: right;
    font-size: 1.4rem;
}
.bscroll-my{
    width: 100%;
    position: absolute;
    overflow: hidden;
    bottom: 50px;
    top: 65px;
    left: 0;
    background:#f3f3f3;
    height: 100%;
}
.bscroll-container{
    height: 105%;
    width: 100%;
    background: #ffffff;
}
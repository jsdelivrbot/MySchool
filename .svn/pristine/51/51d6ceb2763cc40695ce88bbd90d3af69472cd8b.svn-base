<style>
    * {
        -webkit-user-select: none;
        margin: 0;
        padding: 0;
    }

    .wrapper {
        background-image: url("images/bright.png");
    }

    .wuTheme:hover {
        border: 2px solid #a68dff;
    }

    .toolbar-item {
        font-weight: bold;
        font-size: 20px;
        color: #ffffff;
        width: 90%;
        -webkit-border-radius: 10px;
        background-color: #9dafff;
        height: 70px;
        line-height: 70px;
        top: -40px;
        left: -30px;
        position: relative;
        text-indent: 2em;
        box-shadow: 1px 1px 1px 1px #c1c1c1;

    }

    .toolbar {
        font-weight: bold;
        font-size: 20px;
        color: #ffffff;
        width: 90%;
        -webkit-border-radius: 10px;
        background-color: #9dafff;
        height: 70px;
        line-height: 70px;
        top: -10px;
        left: -30px;
        position: relative;
        text-indent: 2em;

        box-shadow: 1px 1px 1px 1px #c1c1c1;
    }

    .closeExam {
        display: inline-block;
        width: 60px;
        height: 35px;
        text-align: center;
        line-height: 35px;
        font-weight: bold;
        font-size: 12px;
        -webkit-border-radius: 5px;
        background-color: #ff938b;
        position: fixed;
        left: 220px;
        bottom: 130px;
        color: #FFFFFF;

        box-shadow: 1px 1px 1px 1px #dbdbdb;
        cursor: pointer;
    }

    .returnTop {
        display: inline-block;
        width: 60px;
        height: 35px;
        text-align: center;
        line-height: 35px;
        font-weight: bold;
        font-size: 12px;

        -webkit-border-radius: 5px;
        background-color: #957de4;
        position: fixed;
        left: 130px;
        bottom: 130px;
        color: #FFFFFF;
        box-shadow: 1px 1px 1px 1px #dbdbdb;
        cursor: pointer;
    }
</style>

<body>
<!--整体内容-->
<div class="wrapper" id="app" style="padding-top: 20px;">

    <!--右半部分内容-->
    <div class="container">
        <!--右半部分主体内容-->
        <div class="content">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-md-12">
                        <div class="card">
                            <div class="content">
                                <div class="toolbar" style="">
                                    <!--        Here you can write extra buttons/actions for the toolbar              -->
                                    排行榜 ▪ {{questionName}}
                                </div>
                                <div class="fresh-datatables">
                                    <table id="datatables" class="table table-striped table-no-bordered table-hover"
                                           cellspacing="0" width="100%" style="width:100%">
                                        <thead>
                                        <tr>
                                            <th>名次</th>
                                            <th>姓名</th>
                                            <th>班级</th>
                                            <th style="box-shadow: 1px 1px 1px 1px #bdbdbd; width: 120px;text-align: center;background-color: #ff938b;color: #ffffff;-webkit-border-top-left-radius: 20px;-webkit-border-top-right-radius: 20px;text-align: center">
                                                成绩
                                            </th>
                                            <th style="text-align: center">评分</th>
                                            <th class="disabled-sorting text-right">趋势</th>
                                        </tr>
                                        </thead>
                                        <tfoot>
                                        <tr>
                                            <th>名次</th>
                                            <th>姓名</th>
                                            <th>班级</th>
                                            <th style="box-shadow: 1px 1px 1px 1px #bdbdbd; width: 120px; text-align: center; background-color: #ff938b;color: #ffffff;-webkit-border-bottom-left-radius: 20px;-webkit-border-bottom-right-radius: 20px;text-align: center">
                                                成绩
                                            </th>
                                            <th style="text-align: center">评分</th>
                                            <th class="disabled-sorting text-right">趋势</th>
                                        </tr>
                                        </tfoot>
                                        <tbody>
                                        <tr class="table-item phb" v-for="(item,index) in dataPM">
                                            <td>{{index+1}}</td>

                                            <td style="">{{item.studentName}}</td>
                                            <td>{{userInfo.className}}</td>
                                            <td style="box-shadow: 1px 1px 1px 1px #bdbdbd;width: 120px;text-align: center; background-color: #ff938b;color: #ffffff;text-align: center">
                                                {{item.score}}分
                                            </td>
                                            <td style="text-align: center">A</td>
                                            <td class="disabled-sorting text-right"><i class="fa fa-times"></i></td>
                                        </tr>


                                        </tbody>
                                    </table>
                                </div>
                            </div><!-- end content-->
                        </div><!--  end card  -->
                    </div> <!-- end col-md-12 -->
                </div> <!-- end row -->
                <footer class="">
                    <div class="container-fluid">
                        <div class="returnTop" title="点击回到顶部">顶部</div>
                        <div class="closeExam" onclick="closeExam()" title="点击返回">返回</div>

                        <p class="copyright pull-right" style="font-size: 14px">
                            &copy; 2018 <a href="#">云校园</a>，梦想带你起航
                        </p>
                    </div>
                </footer>
            </div>


        </div>
        <!--底部-->

    </div>
</div>

<script>
    var lo = "localhost";
    var customId;
    var app = new Vue({
        el: '#app',
        data: {
            dataPM: [],
            userInfo: {},
            questionName: ""
        },
        methods: {
            getDate(){
                $.post("http://" + lo + ":8080/custromjl/selectCustromjlByCustromId", {"customId": customId}, function (date) {
                    console.log(date);
                    app.dataPM = date;
                }, "json")
            }
        },

        created(){
            customId = localStorage.getItem("customId");
            this.questionName = localStorage.getItem("questionName");
            this.getDate();
            var userInfo = localStorage.getItem("user");
            var obj = JSON.parse(userInfo);
            console.log(obj);
            this.userInfo = obj;
        }
    })

</script>
</body>

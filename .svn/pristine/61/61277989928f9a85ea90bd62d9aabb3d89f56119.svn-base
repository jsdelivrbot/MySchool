#AttendanceApp{
    width: 100%;
}
.adApp-main-top{
    display: flex;
    /*column 纵向  row:横向  -reverse 倒过来了*/
    flex-direction: row;
    /*主轴居中  space-around 平分  space-between两头堵死 中间平分*/
    justify-content: space-between;
    /*纵轴对齐方式flex-end*/
    align-items: center;
    flex-wrap: wrap-reverse;
    padding:10px 0; 
}
.adApp-main-top .el-select{
    width: 120px;
}
.adApp-main-top .adApp-mian-top-item{
    margin: 0 auto;
}
.el-button+.el-button{
    margin-left: 0;
}
.adApp-main-stateShow{
    width: 100%;
    height: 30px;
    padding: 10px 0
}
.adApp-main-stateShow .adApp-main-stateShow-center{
    margin: 0 auto;
    display: flex;
    /*column 纵向  row:横向  -reverse 倒过来了*/
    flex-direction: row;
    /*主轴居中  space-around 平分  space-between两头堵死 中间平分*/
    justify-content: center;
    /*纵轴对齐方式flex-end*/
    align-items: center;
    flex-wrap: wrap-reverse;
}
.adApp-main-stateShow .adApp-main-stateShow-center .el-button{
    padding: 7px 11px;
    border: none;
}
.adApp-main-list{
    padding: 14px;
}
.adApp-main-list .adApp-main-list-item{
    width: 100%;
    background: #ffffff;
    overflow: hidden;
    -webkit-border-radius: 6px;
    margin-bottom: 10px;
    border: 1px #d1d1d1 solid;
}
.adApp-main-list .adApp-main-list-item .adApp-main-list-item-img{
    float: left;
    width:50px;
    height: 50px;
    margin: 8px 12px;
}   
.adApp-main-list .adApp-main-list-item .adApp-main-list-item-img img{
    width:50px;
    height: 50px;
    -webkit-border-radius: 100%;
}
.adApp-main-list .adApp-main-list-item .adApp-main-list-item-name{
    float: left;
    margin: 10px 0 10px 10px;
}
.adApp-main-list .adApp-main-list-item .adApp-main-list-item-name span{
    display: inline-block;
    height: 50px;
    line-height: 50px;
    font-size: 1.5rem;
    font-weight: 600;
}
.adApp-main-list .adApp-main-list-item .adApp-main-list-item-time{
    float: right;
    margin: 8px 12px;
}
.adApp-main-list .adApp-main-list-item .adApp-main-list-item-time .adApp-main-list-item-time-AM{
    line-height: 25px;
    font-size: 1.2rem;
    color: #888888;
}
.adApp-main-list .adApp-main-list-item .adApp-main-list-item-time .adApp-main-list-item-time-PM{
    line-height: 25px;
    font-size: 1.2rem;
    color: #888888;
}
.adApp-main-list .adApp-main-list-item .adApp-main-list-item-state{
    float: right;
    margin: 17px 20px 17px 0 ;
}
.adApp-main-list .adApp-main-list-item .adApp-main-list-item-state .el-button{
    padding: 8px 15px;
}
.bscroll-ad{
    width: 100%;
    position: absolute;
    overflow: hidden;
    top: 65px;
    left: 0;
    bottom: 0;  
    background: #f3f3f3;
    /* height: 700px;
    height: 95%; */
}
.bscroll-container-ad{
    /* height: 800px; */
    width: 100%;
    background: #ffffff;
}
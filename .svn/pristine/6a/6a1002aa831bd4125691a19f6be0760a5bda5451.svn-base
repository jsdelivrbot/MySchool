<script>

    var app = new Vue({
        el: "#app",
        data: {
            lessonList: [],
            lessonRi: [],
            date:{},
            lessonTea:[]
        },
        methods: {
            initLessonYi(id){
                $.post("http://localhost:8080/lesson/allById.do", {id: id}, function (result) {
                    console.log(result);
                    app.lessonList = result;
                    for (var i = 0; i < result.length; i++) {
                        app.date = result[i].lessonDate;
                    }
                }, "json");
            }, initLessonRi(){
                $.post("http://localhost:8080/lesson/selectLessonAllByRi.do", "", function (result) {
                    console.log(result);
                    app.lessonRi = result;
                    for (var i = 0; i < result.length; i++) {
                        app.date = result[i].lessonDate;
                    }
                }, "json");
            }, initLessonTea(){
                var teacherId=localStorage.getItem("id");
                var teacherName=localStorage.getItem("name");
                $.post("http://localhost:8080/lesson/allByTea.do", "id="+teacherId, function (result) {
                    console.log(result);
                    for (var i = 0; i < result.length; i++) {
                        if(result[i].teacherIDA!=teacherName){
                            result[i].teacherIDA="";
                            result[i].roomIDA="";
                            result[i].classIDA="";
                            result[i].subjectNameA="";
                        }
                        if(result[i].teacherIDB!=teacherName){
                            result[i].teacherIDB="";
                            result[i].roomIDB="";
                            result[i].classIDB="";
                            result[i].subjectNameB="";
                        }
                        if(result[i].teacherIDC!=teacherName){
                            result[i].teacherIDC="";
                            result[i].roomIDC="";
                            result[i].classIDC="";
                            result[i].subjectNameC="";
                        }
                        if(result[i].teacherIDD!=teacherName){
                            result[i].teacherIDD="";
                            result[i].roomIDD="";
                            result[i].classIDD="";
                            result[i].subjectNameD="";
                        }
                    }
                    app.lessonTea = result;
                }, "json");
            }

        }, created() {
            this.initLessonTea();
        }
    })
</script>


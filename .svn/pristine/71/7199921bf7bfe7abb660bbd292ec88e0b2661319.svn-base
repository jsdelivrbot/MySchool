.item {
    width: 100%;
    height: 200px;
}

* {
    -webkit-user-select: none;
}

.main {
    padding-top: 10px;
    width: 100%;

}

#showInfo {
    box-shadow: 2px 2px 2px 2px rgba(0, 0, 0, 0.212);
    margin: 0;
    color: #ffffff;
    background-color: rgb(189, 103, 103)

}

.main_left {
    transition-timing-function: linear;
    transition-property: width;
    transition-duration: 2s;
    overflow: hidden;
    float: left;
    width: 15%;
    height: 100%;

}

.main_right {
    transition-timing-function: linear;
    transition-property: width;
    transition-duration: 3s;
    padding: 20px;
    float: right;
    width: 84%;
    height: 100%;
    margin-left: 5px;
    border-radius: 5px;

}

#topForm {
    position: absolute;
    right: 0px;
}

.student, .student1 {
    margin: 2px;
    border-bottom-right-radius: 10px;
    border-top-right-radius: 10px;
    width: 100%;
    height: 50px;
    background-color: #5e5e5e;
    color: #ffffff;
    text-align: left;
    text-indent: 1em;
    line-height: 50px;
}

.student:hover {
    background-color: rgb(148, 148, 148);

}

.closeThis {
    display: none;
}

.item_body {
    position: relative;
    margin: 10px;
    width: 80%;
    height: 400px;

    display: inline-block;
}

.table-con {
    width: 100%;

}

.table-con .table-one {
    height: 50px;
    line-height: 50px;
    border-bottom: 1px solid #dddddd;

}

.table-con .table-one td {
    font-size: 14px;
    font-weight: bold;


}

.table-item {

}

.table-con .table-item {
    cursor: pointer;
    height: 40px;
    line-height: 40px;
}

.table-con .table-item td {

}

.progress {
    display: none;
    height: 3px;
}

#bar {
    height: 3px;
}

.modal-body {
    position: relative;
}

canvas {
    left: 0px;
    position: absolute;
}

.jinengzice {
    display: block;
    white-space: nowrap; /* 不换行 */
    overflow: hidden; /* 内容超出宽度时隐藏超出部分的内容 */
    text-overflow: ellipsis; /* 当对象内文本溢出时显示省略标记(...) ；需与overflow:hidden;一起使用。*/; /* 内容超出宽度时隐藏超出部分的内容 */
}

.table-item_name {

    width: 130px;
    display: block;
    white-space: nowrap; /* 不换行 */
    overflow: hidden; /* 内容超出宽度时隐藏超出部分的内容 */
    text-overflow: ellipsis; /* 当对象内文本溢出时显示省略标记(...) ；需与overflow:hidden;一起使用。*/; /* 内容超出宽度时隐藏超出部分的内容 */
}

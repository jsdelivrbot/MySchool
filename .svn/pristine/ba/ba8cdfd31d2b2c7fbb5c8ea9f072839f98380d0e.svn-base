    <script>
        $(function () {
            $("#insertForm").submit(function () {
                return false;
            });
        });
        var myApproval = new Vue({
            el: "#app",
            data: {
                //请加信息列表
                approvalList: {},
                //插入信息中 班主任ID，教员ID
                insertPart: {}
            },
            methods: {
                //绑定下拉框
                bindType: function () {
                    var _this = this;
                    $.post("http://localhost:8080/stuApproval/selectQingJiaType.do",
                        {},
                        function (result) {
                            var res = result.qingJiaType;
                            var temp = "";
                            for (var i = 0; i < res.length; i++) {
                                temp += "<option value='" + res[i].detailID + "'>" + res[i].attrName + "</option>";
                            }
                            $("#qingjia").append(temp);
                        }, "json");
                },
                //绑定学生姓名
                bindStudentName: function () {
                    var _this = this;
                    var data = { 'studentId': 1 };
                    $.post("http://localhost:8080/stuApproval/selectTeacherIdByStudentId.do",
                        data,
                        function (result) {
                            _this.insertPart = result.teacherIdInfo;
                            var res = result.teacherIdInfo.studentName;
                            $("#studentName").attr('value', res);
                        }, "json");
                },
                //学生请假
                insertApproval: function () {
                    var _this = this;
                    var insertdata = new FormData();
                    var approvalType = $("#qingjia").val();
                    var reason = $("#reason").val();
                    var begin = $("#dateinfo").val();
                    var end = $("#dateinfo2").val();
                    var diff = $("#diffDate").val();
                    var drawingImg = $('#drawing').get(0).files[0];

                    insertdata.append("studentId", 1);
                    insertdata.append("drawingImgFile", drawingImg);
                    insertdata.append("approvalType", approvalType);
                    insertdata.append("reason", reason);
                    insertdata.append("diffDate", diff);
                    insertdata.append("beginDate", begin);
                    insertdata.append("endDate", end);
                    insertdata.append("approvalJY", _this.insertPart.teacherId);
                    insertdata.append("approvalBZR", _this.insertPart.headTeacherId);
                    $.ajax({
                        url: "http://localhost:8080/stuApproval/insertApproval.do",
                        type: 'POST',
                        data: insertdata,
                        dataType: 'json',
                        async: false,
                        processData: false,
                        contentType: false,
                        success: function (result) {
                            if (result > 0) {
                                $("#wyqj").modal("hide");
                                _this.getApprovalList();
                            }
                        }
                    });
                },
                //获取学生的请假记录
                getApprovalList: function () {
                    var _this = this;
                    var data = { "studentId": 1 };
                    $.post("http://localhost:8080/stuApproval/selectApprovalByStudentId.do",
                        data,
                        function (result) {
                            _this.approvalList = result.approvalInfoByStudent;
                        }, "json");
                },
                //获取学生的请假记录详情
                approvalDetail: function (index) {
                    var selector1 = $("#approvalImage img");
                    selector1.attr("src", "http://localhost:8080/upload/" + this.approvalList[index].image);
                    var selector2 = $("#approvalReason p");
                    selector2.text(this.approvalList[index].reason);
                }
            },
            created: function () {
                this.bindType();
                this.bindStudentName();
                this.getApprovalList();
            }
        })
    </script>

        </audio>
    </div>
    <!--整体内容-->
    <div class="wrapper" id="app">

        <!--右半部分内容-->
        <div class="main-panel" style="float: none">
            <!--右半部分主体内容-->
            <div class="container-item">
                <div class="top-container" style="position: relative">
                    <p>任务统计进度</p>
                    <div class="btn-group" style="position: absolute;right: 25px;top: 5px">
                        <button type="button" class="btn btn-default dropdown-toggle" data-toggle="dropdown">
                            看板试图
                            <span class="caret"></span>
                        </button>
                        <ul class="dropdown-menu">
                            <li>
                                <a href="#">看板试图</a>
                            </li>
                            <li>
                                <a href="#">列表试图</a>
                            </li>
                        </ul>
                    </div>
                </div>
                <div class="container-item-con container-fluid">
                    <div class="container-item-item">
                        <p>任务进度统计
                            <input type="text" class="container-item-item-text" placeholder="搜索项目">
                        </p>
                    </div>
                    <div class="container-item-item-bottom">
                        <table class="table table-striped">
                            <thead>
                                <tr>
                                    <th class="text-center">项目</th>

                                    <th>开始时间</th>
                                    <th>结束时间</th>
                                    <th>负责人</th>

                                    <th>完成时间</th>
                                    <th>状态</th>
                                    <th>任务进度</th>

                                </tr>
                            </thead>
                            <tbody>
                                <tr @click="modalItem()" v-for="(item,index) in myLaunch">
                                    <td class="text-center" style="line-height: 40px">{{item.taskName}}</td>


                                    <td style="line-height: 40px">{{item.beginDate}}</td>
                                    <td style="line-height: 40px">{{item.endDate}}</td>
                                    <td style="line-height: 40px">
                                        <img class="table-img" :src="item.imgFilePath">
                                    </td>

                                    <td style="line-height: 40px">0000-00-00 00:00:00</td>
                                    <td style="line-height: 40px;color: rgb(255, 187, 0);font-size: 20px;" v-if="item.taskState==0">✘</td>
                                    <td style="line-height: 40px;color: rgb(149, 243, 42);font-size: 20px;" v-if="item.taskState==1">✔</td>
                                    <td style="line-height: 40px;color: rgb(255, 8, 0);font-size: 20px;" v-if="item.taskState==2">✘</td>
                                    <td style="line-height: 40px;width: 20%;" class="jinduTiao">
                                        <!--进度条-->
                                        <div class="progress">
                                            <div class="progress-bar bar-item" role="progressbar" aria-valuenow="60" aria-valuemin="0" aria-valuemax="100" style="width: 40%;">
                                                40%
                                            </div>
                                        </div>
                                    </td>
                                </tr>

                            </tbody>
                        </table>
                    </div>


                </div>
            </div>

        </div>
    </div>
    <!--模态框部分-->
    <div class="modal fade" id="modalItem" aria-hidden="true" role="dialog" aria-labelledby="myModalLabel">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                    <h5 class="modal-title" id="myModalLabel">头部</h5>
                </div>
                <div class="modal-body">

                </div>
                <div class="modal-footer">
                    <button class="btn btn-primary">保存</button>
                    <button class="btn btn-default" data-dismiss="modal" aria-label="Close">取消</button>

                </div>
            </div>
        </div>
    </div>


    <!--加载基本信息-->
    <script>
        var lo = "localhost";
        var userInfoInit;//用户信息
        var btn = true;//导航栏状态
        //var gui = require('nw.gui');
        $(function () {
            notice();
            getUserInfo();
            getTheme();
        });
        //取出主题并设置上
        function getTheme() {
            var getTheme = localStorage.getItem("theme");
            $(".sidebar").attr("data-color", getTheme);
            var imgPath = localStorage.getItem("imgPath");
            $(".sidebar").attr("data-image", imgPath);
            $(".sidebar-background").css("background-image", 'url(' + imgPath + ')');

        }
        //设置通知
        function notice() {
            demo.showNotification('bottom', 'right', 'warning', '温馨提示', '看完错题可点击右上角✘忽略掉本题', 'pe-7s-smile');

        }
        //初始化用户信息&设置
        function getUserInfo() {
            var userInfo = localStorage.getItem("user");
            var obj = JSON.parse(userInfo);
            userInfoInit = obj;
            $(".userName").html(userInfoInit.studentName + '  <b class="caret"></b>')
            imgLogoInit();
        }
        //初始化头像
        function imgLogoInit() {
            $(".photo").html('  <img src="http://localhost:8080/upload/' + userInfoInit.imgFilePath + '.jpg"/>')
        }
        //退出程序
        //退出应用程序
        function clostExamSystem() {
            //gui.App.quit()
        }

    </script>

    <!--VUE模块-->
    <script>

        var app = new Vue({
            el: '#app',
            data: {
                userInfo: {},
                myLaunch: []
            },
            methods: {
                modalItem() {
                    $('#modalItem').modal('show')
                },
                getMyLaunchTask() {
                    //获取我发起的任务 未完成的
                    $.post("http://" + lo + ":8080/Task/selectTaskbyStudent", { "studentId": this.userInfo.studentId }, function (data) {
                        app.myLaunch = data;
                        for (var i = 0; i < data.length; i++) {
                            data[i].imgFilePath = 'http://localhost:8080/upload/' + data[i].imgFilePath + '.jpg';
                        }
                        console.log(data);
                        console.log(data.length);
                    }, "json");
                },
                getUserInfo() {
                    var userInfo = localStorage.getItem("user");
                    var obj = JSON.parse(userInfo);
                    this.userInfo = obj;
                    this.getMyLaunchTask();
                }
            },
            created() {
                this.getUserInfo();
            }
        })

    </script>

</body>

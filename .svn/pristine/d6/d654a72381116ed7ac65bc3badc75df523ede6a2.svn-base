.main_right .main_right_item {
    width: 100%;
    height: 100%;

}

.main_right .timeRemaining {
    width: 190px;
    height: 70px;
    text-align: center;
    line-height: 70px;
    font-size: 34px;
    color: #ffffff;
    -webkit-border-bottom-left-radius: 10px;
    -webkit-border-top-left-radius: 10px;
    position: absolute;
    right: 3px;
    top: 10px;
    background-color: #d25688;
    box-shadow: 2px 2px 2px 1px #bc4b7a;
}

.title {

    background-color: #FFFFFF;

    -webkit-border-top-left-radius: 5px;
    -webkit-border-top-right-radius: 5px;
    width: 100%;
    padding-top: 25px;
    padding-bottom: 25px;
    display: flex;
    /*column 纵向  row:横向  -reverse 倒过来了*/
    flex-direction: row;
    /*主轴居中  space-around 平分  space-between两头堵死 中间平分*/
    justify-content: space-between;
    /*纵轴对齐方式flex-end*/
    align-items: center;
    flex-wrap: wrap-reverse;
    text-indent: 2em;
    border-top: 1px solid #ece9ea;
    border-left: 1px solid #ece9ea;
    border-bottom: 1px solid #e8e8e8;
    box-shadow: 2px 2px 2px 1px #ece9ea;
}

.title .main_right_title {
    font-size: 18px;
    width: 80%;

    padding-left: 20px;
}

.answer {
    position: relative;
    width: 100%;
    padding-bottom: 200px;
    background-color: #FFFFFF;
    border-left: 1px solid #eaeaea;
    border-bottom: 1px solid #eaeaea;
    -webkit-border-bottom-left-radius: 5px;

}

.answer p {
    text-indent: 4em;
    line-height: 50px;
    background-color: #f8ffea;
    -webkit-border-radius: 5px;
    width: 100%;
    height: 50px;
    cursor: pointer;
}

.answer p:hover {
    background-color: #f7f3e4;
}

.bottomCao {

    padding-right: 5px;
    text-align: right;
    position: absolute;
    bottom: 30px;
    right: 20px;

    height: 70px;
    text-indent: 0em;

}

.onOrDuo {
    position: absolute;
    width: 50px;
    height: 25px;
    background-color: #4d994d;
    text-align: center;
    line-height: 25px;
    font-size: 10px;
    color: #ffffff;
    font-weight: bold;
    -webkit-border-bottom-right-radius: 4px;
}

.main_left_top {
    position: relative;
}

canvas {

    position: absolute;
    top: 0px;
    left: 0px;
}

.jinengzice {
    display: block;
    white-space: nowrap; /* 不换行 */
    overflow: hidden; /* 内容超出宽度时隐藏超出部分的内容 */
    text-overflow: ellipsis; /* 当对象内文本溢出时显示省略标记(...) ；需与overflow:hidden;一起使用。*/; /* 内容超出宽度时隐藏超出部分的内容 */
}


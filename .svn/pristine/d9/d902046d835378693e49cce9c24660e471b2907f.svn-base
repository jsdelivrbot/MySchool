* {
    -webkit-user-select: none;
}

.iframeItem {
    width: 100%;
    height: 100%;
}

.wuTheme:hover {
    border: 2px solid #a68dff;
}

#allmap {
    -webkit-border-radius: 10px;
    width: 100%;
    height: 100%;
}

.toolbar-item {
    font-weight: bold;
    font-size: 20px;
    color: #ffffff;
    width: 90%;

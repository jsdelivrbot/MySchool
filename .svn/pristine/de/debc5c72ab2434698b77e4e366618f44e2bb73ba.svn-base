.item {
    width: 100%;
    height: 200px;
}

* {
    margin: 0px;
    padding: 0px;
    -webkit-user-select: none;
}

.main {
    padding-top: 10px;
    width: 100%;

}

#showInfo {
    box-shadow: 2px 2px 2px 2px rgba(0, 0, 0, 0.212);
    margin: 0;
    color: #ffffff;
    background-color: #BD6767;

}

.main_left {

    transition-timing-function: linear;
    transition-property: width;
    transition-duration: 2s;
    overflow: hidden;
    float: left;
    width: 15%;
    height: 100%;

}

a {
    text-decoration: none;
}

.main_right {
    transition-timing-function: linear;
    transition-property: width;
    transition-duration: 3s;
    padding: 20px;
    float: right;
    width: 84%;
    height: 485px;
    margin-left: 5px;
    border-radius: 5px;
    background-color: #feffd0;

}

#topForm {

}

.student, .student1 {
    margin: 2px;
    border-bottom-right-radius: 10px;
    border-top-right-radius: 10px;
    width: 100%;
    height: 50px;
    background-color: #5e5e5e;
    color: #ffffff;
    text-align: left;
    text-indent: 1em;
    line-height: 50px;
}

.student:hover, .student1:hover {
    background-color: rgb(148, 148, 148);

}

.closeThis {
    display: none;
}

.item_body {
    position: relative;
    margin: 10px;
    width: 80%;
    height: 400px;

    display: inline-block;
}

video {

    width: 100%;
    height: 100%;
    padding-left: 10px;
    padding-right: 10px;
    padding-bottom: 20px;

    border-radius: 10px;
    background-color: #3c3c3c;
}

.video_p {
    padding-left: 10px;
    text-align: center;
    position: absolute;
    top: 8px;
    font-size: 14px;
    text-indent: 3px;
    font-weight: bold;
    color: #ffffff;
}

.icon-weibiaoti10 {
    cursor: pointer;
}

.icon-tuichuquanping-copy {
    cursor: pointer;
}

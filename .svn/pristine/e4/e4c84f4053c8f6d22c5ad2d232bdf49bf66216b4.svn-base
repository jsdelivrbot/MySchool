<script>
    /*三级联动，绑定下拉菜单的数据*/
    $(function () {
        $.post("http://localhost:8080/skill/selFather.do", {}, function (res) {
            var data = eval(res);
            var temp = "";
            for (var i = 0; i < data.length; i++) {
                temp += "<option value='" + data[i].skillId + "'>" + data[i].skillName + "</option>";
            }
            $("#firstMenu option").remove();
            $("#firstMenu").append(temp);
        });
        /*绑定二级菜单*/
        $("#firstMenu").change(function () {
            var fatherId = $("#firstMenu").val();
            $.post("http://localhost:8080/skill/getChildMenu.do", { fatherId: fatherId }, function (res) {
                var data = eval(res);
                var temp = "";
                for (var i = 0; i < data.length; i++) {
                    temp += "<option value='" + data[i].skillId + "'>" + data[i].skillName + "</option>";
                }
                $("#secondMenu option").remove();
                $("#secondMenu").append(temp);
            });
        });
        /*绑定三级菜单*/
        $("#secondMenu").change(function () {
            var fatherId = $("#secondMenu").val();
            $.post("http://localhost:8080/skill/getChildMenu.do", { fatherId: fatherId }, function (res) {
                var data = eval(res);
                var temp = "";
                for (var i = 0; i < data.length; i++) {
                    temp += "<option value='" + data[i].skillId + "'>" + data[i].skillName + "</option>";
                }
                $("#thirdMenu option").remove();
                $("#thirdMenu").append(temp);
            });
        });
        /*获取三级菜单最终的知识点编号，去查询对应的试题*/
        $("#thirdMenu").change(function () {
            var skillId = $("#secondMenu").val();
            alert(skillId);
            $.post("http://localhost:8080/topic/getTopicListBySkillId.do", { skillId: skillId }, function (res) {
                var data = eval(res);
                var i = 0;
                /*下一题能用*/
                $("#next").click(function () {
                    var time = new Date();
                    alert(time);
                    if (i < res.length) {
                        i += 1;
                    } else {
                        i = data.length;
                    }
                    $("#questionDetails").text(data[i].topicName);
                    $("#analysis").text(data[i].modelAnswer);
                    return false;
                });
                /*上一题能用*/
                $("#last").click(function () {
                    if (i >= 1) {
                        i--;
                        $("#questionDetails").text(data[i].topicName);
                        $("#analysis").text(data[i].modelAnswer);
                        return false;
                    } else {
                        i = 0;
                        $("#questionDetails").text(data[i].topicName);
                        $("#analysis").text(data[i].modelAnswer);
                        return false;
                    }
                });
                /*页面加载时，绑定初始数据*/
                $("#questionDetails").text(data[i].topicName);
                $("#analysis").text(data[i].modelAnswer);
            });
        });
    })
</script>


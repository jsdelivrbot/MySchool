<script>
    //http://localhost:8080/evaluation/getCommonsByStudentId.do
    $(function () {
        var data = localStorage.getItem("user");
        var studentId = JSON.parse(data).studentId;
        $("#studentId").val(studentId);
        $.post("http://localhost:8080/evaluation/getCommonsByStudentId.do", { studentId: studentId }, function (res) {
            var data = eval(res);
            var temp = "";
            for (var i = 0; i < data.length; i++) {
                temp += "<div class='padd'>" + data[i].comments + "<span class='time'>" + data[i].evaluationDate + "</span> <p> 谢谢老师鼓励 </p> </div> <hr/>";
            }
            $("#show div,#show hr").remove();
            $("#show").append(temp);
        });
    });

    function btnQuery() {
        var param = $("#form").serialize();
        $.post("http://localhost:8080/evaluation/getCommonsByStudentId.do", param, function (res) {
            var data = eval(res);
            var temp = "";
            for (var i = 0; i < data.length; i++) {
                temp += "<div class='padd'>" + data[i].comments + "<span class='time'>" + data[i].evaluationDate + "</span> <p> 谢谢老师鼓励 </p> </div> <hr/>";
            }
            $("#show div,#show hr").remove();
            $("#show").append(temp);
        });
    }
</script>

